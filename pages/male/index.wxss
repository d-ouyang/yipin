.tryst-item{
  margin-bottom: 32rpx;
}
 
.item-box {
  box-sizing: border-box;
  padding: 40rpx;
  border-radius: 8rpx;
  background: #fff;
  height: 300rpx;
  justify-content: space-between;
}

.item-title {
  margin-bottom: 12rpx;
  font-weight: bold;
  font-size: 32rpx;
}

.item-content {
  font-size: 28rpx;
  margin-bottom: 40rpx;
}

.submit-solution {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
  background-color: #F7291B;
}

.female-entrence {
  position: fixed;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  left: 40rpx;
  bottom: 140rpx;
  border-radius: 50%;
  background-color: #1A1A1A;
  color: #fff;
}

.setting {
  position: fixed;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  right: 40rpx;
  bottom: 140rpx;
  border-radius: 50%;
  background-color: #1A1A1A;
  color: #fff;
}